<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  <package name="forward" extends="struts-default" namespace="/">
  		<action name="forwardAction_*" method="{1}" class="com.zhangying.oa.struts2.action.ForwordAction">
  			<result name="left">/WEB-INF/frame/left.jsp</result>
  			<result name="right">/WEB-INF/frame/right.jsp</result>
  			<result name="top">/WEB-INF/frame/top.jsp</result>
  			<result name="bottom">/WEB-INF/frame/bottom.jsp</result>
  		</action>
  </package>
</struts>	